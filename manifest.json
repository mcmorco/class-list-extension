{
    "manifest_version": 3,
    "name": "Zoom Meeting Shortcut",
    "permissions": ["activeTab","identity","storage"],
    "description": "An extension that contains Zoom meeting details of personal classes.",
    "version": "3.0.1",
    "icons": {
        "16":"/images/dev-img.png",
        "48":"/images/dev-img.png",
        "128":"/images/dev-img.png"
    },
    "action":{
        "default_title": "Time to go to class!",
        "default_popup": "popup.html"
    }
}
